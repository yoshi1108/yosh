          <BSFPreProcessor guiclass="TestBeanGUI" testclass="BSFPreProcessor" testname="BSF PreProcessor" enabled="true">
            <stringProp name="scriptLanguage">javascript</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">var ctrl_count_1 = 0;  //現在のコントロータのシリアス番号。初期値０．
var ctrl_id = &quot;&quot;;  //現在のコントローラのctrl_id文字列
var ctrl_id_array = &quot;&quot;;  //検索用ctrl_idの集合

//前回保存された変数からctrl_count_1の値を取得。保存された値がない場合は、初期値０になる
if (vars.get(&apos;ctrl_count_1&apos;)) {
	ctrl_count_1=vars.get(&apos;ctrl_count_1&apos;);
}

//以下の処理を繰り返し、検索用ctrl_id文字列を生成

//ループの開始位置を記録
var loop_begin = ctrl_count_1;
//ループ回数を定義する。総回数n回のうち、最初のn-1回で固定値loop_repeat回繰り返す。最後の1回で残りのコントローラ分繰り返す。
var loop_num = ((${cont_num}-loop_begin)&lt;${loop_repeat}) ? ${cont_num}%${loop_repeat} : ${loop_repeat};
//ループ開始
for (var i=1;i&lt;=loop_num;i++) {
	ctrl_count_1++;
	//log.info(&quot;★★★★ctrl_count_1: &quot;+ctrl_count_1);
	//決めたフォーマットに従ってctrl_idを生成する。
	ctrl_id = &quot;999&quot;+&quot;-&quot;+(&quot;000000000&quot;+ctrl_count_1).slice(-10);
	//生成したctrl_idを検索用文字列ctrl_id_arrayの末尾に追加する。
	ctrl_id_array=ctrl_id_array+&quot;,&quot;+ctrl_id;
}
//最初の余分な「,」を切り捨て
ctrl_id_array=ctrl_id_array.slice(1);
log.info(&quot;★★★★ctrl_id IN: &quot;+ctrl_id_array);

//変数に保存
vars.put(&apos;ctrl_count_1&apos;, ctrl_count_1);
vars.put(&apos;ctrl_id_array&apos;, ctrl_id_array);
</stringProp>
          </BSFPreProcessor>
